digraph test_attr_01 {
	label = "这个是标题";
	fontsize = 10; // 字体大小
	rankdir = LR;  // 方向是从左到右

	node [ shape = box, color = green,
		style = filled,
		fillcolor = "0.7 0.3 1.0",
		fontsize = 10 ];

